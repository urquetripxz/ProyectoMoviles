        <activity
            android:name=".NegociosFragment"
            android:label="@string/title_activity_negocios_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NegociosActivity"
            android:label="@string/title_activity_negocios"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEditNegocioActivity"
            android:label="@string/title_activity_add_edit_negocio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NegocioDetailActivity"
            android:label="@string/title_activity_negocio_detail"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>